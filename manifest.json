{
  "name": "Set Headers",
  "description": "Modify the headers",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon_16x16.png",
    "32": "icons/icon_32x32.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "action": {
    "default_popup": "dist/popup.html",
